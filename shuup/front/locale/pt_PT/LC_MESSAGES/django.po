msgid ""
"<p>Dear {{ order.customer }},</p>\n"
"<p>A shipment has been created for your order and here are the details:</p>\n"
"\n"
"{% if shipment.tracking_code %}\n"
"<p>Tracking code: {{ shipment.tracking_code }}</p>\n"
"{% endif %}\n"
"\n"
"<p>Contents:</p>\n"
"<table>\n"
"    <thead>\n"
"        <tr>\n"
"            <th>Product</th>\n"
"            <th>Quantity</th>\n"
"        </tr>\n"
"    </thead>\n"
"    <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"        <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"        </tr>\n"
"{% endif %}\n"
"    </tbody>\n"
"</table>\n"
msgstr "<p>Caro {{ encomenda.cliente }},</p>\n<p>Um envio foi criado para a sua encomenda e aqui estão os detalhes:\n</p>\n\n{% se envio.rastreio_código %}\n<p>Código de rastreio: {{ envio.rastreio_código }}</p>\n{% terminarse %}\n\n<p>Conteúdos:</p>\n<table>\n<thead>\n<tr>\n<th>Produto</th>\n<th>Quantidade</th>\n</tr>\n</thead>\n<tbody>\n{% para envio_produto em envio.total_produtos %}\n<tr>\n<td>{{ envio_produto.produto }}</td>\n<td>{{ envio_produto.quantidade }}</td>\n</tr>\n{% terminarse %}\n</tbody>\n</table>\n"

