msgid ""
"<p>Dear {{ order.customer }},</p>\n"
"<p>A shipment has been created for your order and here are the details:</p>\n"
"\n"
"{% if shipment.tracking_code %}\n"
"<p>Tracking code: {{ shipment.tracking_code }}</p>\n"
"{% endif %}\n"
"\n"
"<p>Contents:</p>\n"
"<table>\n"
"    <thead>\n"
"        <tr>\n"
"            <th>Product</th>\n"
"            <th>Quantity</th>\n"
"        </tr>\n"
"    </thead>\n"
"    <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"        <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"        </tr>\n"
"{% endif %}\n"
"    </tbody>\n"
"</table>\n"
msgstr ""
"<p>Estimado {{ order.customer }}: </p>\n"
"<p>Se ha creado un envío para su pedido y estos son los detalles:</p>\n"
"\n"
"{% if shipping.tracking_code %}\n"
"<p> Código de seguimiento: {{ shipping.tracking_code }} </p>\n"
"{% endif %}\n"
"\n"
"<p> Contenido: </p>\n"
"<tabla>\n"
"     <thead>\n"
"         <tr>\n"
"             <th>Producto</th>\n"
"             <th>Cantidad</th>\n"
"         </tr>\n"
"     </thead>\n"
"     <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"         <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"         </tr>\n"
"{% endif %}\n"
"     </tbody>\n"
"</table>\n"

msgid "Send Shipment Created Email"
msgstr "Enviar correo electrónico de envío creado"

msgid "Send an email to customer when a shipment is created."
msgstr "Envíe un correo electrónico al cliente cuando se cree un envío."

msgid ""
"This script will send an email to customer when a shipment of his order has "
"been created."
msgstr ""
"Esta secuencia de comandos enviará un correo electrónico al cliente cuando "
"se haya creado un envío de su pedido."

msgid "{{ order.shop }} - Shipment created for order {{ order.identifier }}"
msgstr "{{ order.shop }}: envío creado para el pedido {{ order.identifier }}"

msgid "Send Shipment Deleted Email"
msgstr "Enviar correo electrónico eliminado del envío"

msgid "Send email when a shipment is deleted."
msgstr "Envíe un correo electrónico cuando se elimine un envío."

msgid ""
"This script will send an email to customer or to any configured email right "
"after a shipment gets deleted."
msgstr ""
"Esta secuencia de comandos enviará un correo electrónico al cliente o a "
"cualquier correo electrónico configurado inmediatamente después de que se "
"elimine un envío."

msgid "{{ order.shop }} - Shipment deleted for order {{ order.identifier }}"
msgstr ""
"{{ order.shop }}: envío eliminado para el pedido {{ order.identifier }}"

msgid "Send Payment Created Email"
msgstr "Enviar correo electrónico de pago creado"

msgid "Send email to customer when a payment is created."
msgstr "Envíe un correo electrónico al cliente cuando se cree un pago."

msgid ""
"This script will send an email to customer or to any configured email right "
"after a payment gets created."
msgstr ""
"Esta secuencia de comandos enviará un correo electrónico al cliente o a "
"cualquier correo electrónico configurado inmediatamente después de que se "
"cree un pago."

msgid "{{ order.shop }} - Payment created for order {{ order.identifier }}"
msgstr "{{ order.shop }}: Pago creado para el pedido {{ order.identifier }}"

msgid "Send Refund Created Email"
msgstr "Enviar correo electrónico de reembolso creado"

msgid "Send email when a refund is created."
msgstr "Envíe un correo electrónico cuando se cree un reembolso."

msgid ""
"This script will send an email to customer or to any configured email right "
"after a refund gets created."
msgstr ""
"Esta secuencia de comandos enviará un correo electrónico al cliente o a "
"cualquier correo electrónico configurado inmediatamente después de que se "
"cree un reembolso."

msgid "{{ order.shop }} - Refund created for order {{ order.identifier }}"
msgstr ""
"{{ order.shop }}: Reembolso creado para el pedido {{ order.identifier }}"

msgid "Send Order Confirmation Email"
msgstr "Enviar correo electrónico de confirmación de pedido"

msgid "Send a confirmation email when the order is created."
msgstr "Envíe un correo electrónico de confirmación cuando se cree el pedido."

msgid ""
"This script will send an email to customer or to any configured email right "
"after an order is created. The order contents can be put on email body as "
"well as other informations like shipping method and payment method."
msgstr ""
"Esta secuencia de comandos enviará un correo electrónico al cliente o a "
"cualquier correo electrónico configurado inmediatamente después de que se "
"crea un pedido. El contenido del pedido se puede poner en el cuerpo del "
"correo electrónico, así como otra información como el método de envío y el "
"método de pago."

