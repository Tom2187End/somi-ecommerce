msgid ""
"<p>Dear {{ order.customer }},</p>\n"
"<p>A shipment has been created for your order and here are the details:</p>\n"
"\n"
"{% if shipment.tracking_code %}\n"
"<p>Tracking code: {{ shipment.tracking_code }}</p>\n"
"{% endif %}\n"
"\n"
"<p>Contents:</p>\n"
"<table>\n"
"    <thead>\n"
"        <tr>\n"
"            <th>Product</th>\n"
"            <th>Quantity</th>\n"
"        </tr>\n"
"    </thead>\n"
"    <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"        <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"        </tr>\n"
"{% endif %}\n"
"    </tbody>\n"
"</table>\n"
msgstr "<p>Estimado {{ order.customer }},</p>\n<p>Un envío ha sido creado para tu pedido, y aquí están los detalles: </p>\n\n{% if shipment.tracking_code %}\n<p>Tracking code: {{ shipment.tracking_code }}</p>\n{% endif %}\n\n<p>Contenidos:</p>\n<table>\n    <thead>\n        <tr>\n            <th>Producto</th>\n            <th>Cantidad</th>\n        </tr>\n    </thead>\n    <tbody>\n{% for shipment_product in shipments.total_products %}\n        <tr>\n            <td>{{ shipment_product.product }}</td>\n            <td>{{ shipment_product.quantity }}</td>\n        </tr>\n{% endif %}\n    </tbody>\n</table>\n"

msgid "Send Shipment Created Email"
msgstr "Enviar email de envío creado"

msgid "Send an email to customer when a shipment is created."
msgstr "Enviar un email al cliente cuando un envío es creado."

msgid ""
"This script will send an email to customer when a shipment of his order has "
"been created."
msgstr "Este script enviará un correo electrónico al cliente cuando se haya creado un envío de su pedido."

msgid "{{ order.shop }} - Shipment created for order {{ order.identifier }}"
msgstr "{{ pedido.tienda }} - Envío creado para el pedido {{ pedido.identificador }}"

msgid "Send Shipment Deleted Email"
msgstr "Enviar correo electrónico eliminado del envío"

msgid "Send email when a shipment is deleted."
msgstr "Enviar email cuando un envío es eliminado."

