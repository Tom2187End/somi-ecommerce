msgid "Value per unit: %(value)s"
msgstr "Стоимость единицы: %(value)s"

#, python-format
msgid "Total value: %(value)s"
msgstr "Общая стоимость: %(value)s"

#, python-format
msgid "Alert limit: %(limit)s"
msgstr "Предел количества, для вывода предупреждения: %(limit)s"

